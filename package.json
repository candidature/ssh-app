{
  "name": "ssh-app",
  "version": "1.0.0",
  "description": "A ssh2 client application based on electron",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist out tmp build",
    "electron:dev": "cross-env DEV=1 electron app",
    "pack": "build --dir",
    "postinstall": "install-app-deps",
    "start": "run-p webpack:start electron:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:build": "cross-env NODE_ENV=production webpack -p --progress",
    "webpack:start": "webpack-dev-server --progress --hot --port 4200"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nileshmali/ssh-app.git"
  },
  "keywords": [
    "ssh2",
    "electron",
    "desktop",
    "angular2"
  ],
  "author": "Nilesh Mali",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nileshmali/ssh-app/issues"
  },
  "homepage": "https://github.com/nileshmali/ssh-app#readme",
  "devDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@ngtools/webpack": "1.0.0",
    "@types/electron": "1.4.23",
    "@types/node": "6.0.43",
    "@types/ssh2": "0.5.33",
    "@types/ssh2-streams": "0.1.2",
    "@types/webpack": "1.12.35",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "1.0.0-beta.0",
    "copy-webpack-plugin": "3.0.1",
    "core-js": "2.4.1",
    "cross-env": "3.1.1",
    "css-loader": "0.25.0",
    "devtron": "1.3.0",
    "electron": "1.4.2",
    "electron-builder": "7.10.2",
    "electron-debug": "1.0.1",
    "eslint": "3.7.1",
    "file-loader": "0.9.0",
    "font-awesome": "4.6.3",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.0",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "4.0.2",
    "script-loader": "0.7.0",
    "ssh2": "0.5.2",
    "style-loader": "0.13.1",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "uglify-js": "mishoo/UglifyJS2#harmony",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.8",
    "webpack-merge": "0.14.1",
    "zone.js": "0.6.25"
  },
  "build": {
    "appId": "com.github.ssh2",
    "asar": false,
    "extraResources": [
      {
        "from": "build",
        "to": "app"
      }
    ],
    "linux": {
      "target": [
        "deb"
      ]
    }
  }
}
